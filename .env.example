# Rename this file to .env and fill in your values
DISCORD_TOKEN=your-bot-token-here
# Postgres URL, e.g. postgres://user:pass@host:5432/dbname
DATABASE_URL=postgres://user:pass@localhost:5432/discordbot
# Default timezone for human-friendly scheduling (IANA tz name)
DEFAULT_TZ=Europe/Berlin
# Your Discord user id for owner-only commands
BOT_OWNER_ID=123456789012345678
# Optional: DeepL API key for translations; if unset, German text is sent as-is
DEEPL_KEY=
# Optional: GitHub token + repo for committing features.json
GITHUB_TOKEN=
GITHUB_REPO=yourname/yourrepo  # e.g., username/repo
GITHUB_BRANCH=main
GITHUB_TOKEN_EXPIRATION=2025-11-05
